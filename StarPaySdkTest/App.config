<?xml version="1.0" encoding="utf-8" ?>
<configuration>
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
  </startup>
  <connectionStrings>
    <add name="DefaultConnection" connectionString="server=(local);uid=sa;pwd=123456;database=UplinkCar;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <appSettings>
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <!--图片服务器上传接口-->
    <add key="custom:UploadServerUrl" value="http://upload.uplink.com/Api/UploadImage" />
    <!--WebApp服务器地址-->
    <add key="custom:WebAppServerUrl" value="http://localhost:16665" />
    <!--WebApp静态资源路径-->
    <add key="custom:WebAppStaticResourceServerUrl" value="http://res.uplink.com/WebMobile" />
    <add key="custom:RedisServer" value="127.0.0.1:6379" />
    <add key="custom:IsTest" value="true" />
  </appSettings>
