                            if(d.length==0) {
                                $.lumos.tips('至少勾选一种险种');
                            }
                            else{

                                for(var i=0;i<d.length;i++){

                                    var kind=$.lumos.jsonHelper.getDetail(offerKinds,"kindId",d[i].kindId);
