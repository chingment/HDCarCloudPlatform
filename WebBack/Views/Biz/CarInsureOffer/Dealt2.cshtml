            $('#btn_select_kinds').live('click', function () {

                var selectedIds='';

                for(var i=0;i<offerKinds.length;i++){
                    selectedIds+=offerKinds[i].kindId+',';
                }


                var dialog = art.dialog.open('/Biz/CarInsureOffer/SelectKinds?selectedIds='+selectedIds, {
